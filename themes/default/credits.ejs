<script>
  // Function to check if daily coins can be claimed and update the card
  async function checkDailyCoins() {
    try {
      const response = await fetch('/api/dailycoins');
      const data = await response.json();

      const coinCard = document.getElementById('coinCard');
      const coinTitle = document.getElementById('coinTitle');
      const coinInfo = document.getElementById('coinInfo');
      const claimButton = document.getElementById('claimButton');

      if (data.canClaim) {
        // Coins can be claimed
        coinCard.style.backgroundColor = '#191c1f'; // Change the card background color if desired
        coinInfo.style.color = '#68717c'; // Change the text color if desired
        claimButton.style.display = 'block';
        coinInfo.innerHTML = 'You can claim your daily 250 credits now!';
      } else {
        // Coins cannot be claimed, display timer and hide the claim button
        coinCard.style.backgroundColor = '#191c1f'; // Change the card background color if desired
        coinInfo.style.color = '#68717c'; // Change the text color if desired
        coinTitle.style.color = 'white'; // Change the text color to light gray
        claimButton.style.display = 'none'; // Hide the claim button
        const timeUntilClaim = formatTime(data.timeUntilClaim);
        coinInfo.innerHTML = `You can claim again in ${timeUntilClaim}.`;
      }
    } catch (error) {
      console.error(error);
    }
  }

// Function to claim coins and hide the claim button
async function claimCoins() {
  try {
    const response = await fetch('/claim', { method: 'GET' });
    const data = await response.json();
    if (response.status === 200) {
      // Successful claim, update the card
      const coinInfo = document.getElementById('coinInfo');
      coinInfo.innerHTML = 'Claimed successfully!';
      const claimButton = document.getElementById('claimButton');
      claimButton.style.display = 'none'; // Hide the claim button
    } 
  } catch (error) {
    console.error(error);
  }
}

  // Helper function to format time in short format (e.g., "6h 30m")
  function formatTime(milliseconds) {
    const hours = Math.floor(milliseconds / 3600000);
    const minutes = Math.floor((milliseconds % 3600000) / 60000);
    return `${hours}h ${minutes}m`;
  }

  // Call the checkDailyCoins function when the page loads
  window.onload = checkDailyCoins;
</script>


